<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="mybatis.jobMapper">

	<insert id="Write" parameterType="jobDTO">	
	 	insert into RM_introduction values  
		(RM_job_SEQ.nextval,#{rmj_job},#{rmj_career},#{rmj_carrerStart},#{rmj_carrerEnd},
		#{rmj_type}) 
	</insert>	
	<select id="selectLastSeq" resultType="int">
		select max(rmj_seq) from rm_job
	</select>
</mapper>