<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="mybatis.foreignMapper">
	<insert id="foreignWrite" parameterType="foreignDTO">
		insert into RS_portfolio
		values (#{rsfSeq} , #{rsfName} , #{rsfStartdate},
		#{rsfEnddate} , #{rsfContent} , #{mId})
	</insert>
	
	<delete id="foreignDelete" parameterType="int">
		delete RS_foreign where RSF_seq = #{rsfSeq};
	</delete>
</mapper>