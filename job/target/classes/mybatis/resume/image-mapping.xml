<mapper namespace="mybatis.imageMapper">
	
	 <insert id="imageWrite" parameterType="imageDTO">
	 	insert into RS_image values 
		(RS_image_SEQ.nextval, #{rsimName}, #{mId}, #{rsimUsertitle});
	 </insert>
		
